<% const userImage = userInfo.profileImage.replace(/\\/g, "/") %>

<i class="fas fa-chevron-circle-up recc-show fa-3x"></i>
    <div id="primary" class="content-area">
      <main id="main" class="site-main">

        <div class="set-wrap">
        <div class="set">

          <div class="field-wrap">
            <div class="field-info">
              <input class="top_input" type="text" name="" value="" placeholder="Search for albums..." />
              <i class="far fa-plus-square add-btn fa-2x"></i>
              <!-- <form class="input-form">
                <p class="click-to-add">Click to add a custom album</p>
                <i class="far fa-plus-square add-btn"></i>
              </form> -->
            </div>

            <fieldset>
              <div class="input_suggs">
              </div>

              <div class="round-down-btn round-hide">
                <i class="fas fa-chevron-up fa-2x"></i>
              </div>
            </fieldset>
          </div>
          <div class="tile-opts">
            <button class="save field-btn" type="button" name="button">Save</button>
            <button class="clear field-btn" type="button" name="button">New</button>
            <div class="numbers-wrap">
              <p>Numbers?</p> <input id="desk-numRadio" type="checkbox" checked="true" class="chartNums" name="chartNums" value="true" />
            </div>
          </div>
          <div class="saved"></div>
        </div>
      </div>

        <section class="display-screen">

        <div class="display">
          <div class="top-radio-wrap" data-html2canvas-ignore="true">
            <input class="top-select" type="radio" id="top-ten" name="top-select" value="top-ten">
            <label for="top-ten" class="top-select">Top 10</label><br>
            <input class="top-select" type="radio" id="top-twenty" name="top-select" value="top-twenty">
            <label for="top-twenty" class="top-select">Top 20</label><br>
            <input class="top-select" type="radio" id="top-fifty" name="top-select" value="top-fifty">
            <label for="top-fifty" class="top-select">Top 50</label><br>
            <input class="top-select" type="radio" id="top-hundred" name="top-select" value="top-hundred">
            <label for="top-hundred" class="top-select">Top 100</label><br>
        </div>
          <div class="title-wrap">
            <div class="chart_title">
              <h3>Chart Title:</h3>
              <i class="far fa-edit edit" data-html2canvas-ignore="true"></i>
              <input class="title-change" type="text">
            </div>

            <div>
                <button class="screencap-btn prim-btn" type="submit">Download Img</button>
            </div>          
          </div>
          <div class="top_wrapper">

          </div>
        </div>

        <div class="chart_names">

        </div>
      </section>


      </main>
      <!-- #main -->
    </div>

    <div class="album_reccs">
      <img class="sugg-loader" src="http://localhost:8888/topsters3 - copy/assets/loader.gif" alt="">
    </div>

    <div class="yt-wrap"></div>

<div class="mobile-controls">

    <div class="mobile-nav">
      <div class="mobile-nav-wrap">
        <div>
          <i class="fas fa-list fa-2x"></i>
          <i class="fas fa-music fa-2x"></i>
        </div>

        <div class="round-up">
          <i class="fas fa-chevron-up fa-2x"></i>
        </div>

        <div>
          <i class="fas fa-cog fa-2x"></i>
          <div id="profile-img-display" class="prof-img-mob" style="background-image: url('http://localhost:4001/<%= userImage %>')"></div>
        </div>

      </div>
    </div>

    <div class="mob-settings mob-slider profile-slide">
      <ul>
        <li class="dash-li"><a href="/dashboard">Dashboard</a></li>
        <li class="profile-li"><a href="/<%= userInfo.username %>">Profile</a></li>
        <li class="settings-li"><a href="/settings">Settings</a></li>
        <li class="logout-li"><a href="/logout">Logout</a></li>
      </ul>
    </div>

    <div class="mob-settings mob-slider setting-slide">
      <ul>
        <li class="save">Save</li>
        <li class="clear">New</li>
        <label for="mob-numRadio"><li>Numbers? <input id="mob-numRadio" type="checkbox" checked="true" class="chartNums" name="chartNums" value="true" /></li></label>
        <li class="screencap-btn">Download Image</li>
      </ul>
    </div>
</div>
    

