<header>
  <div class="head-wrap">
    <a class="logo-anc" href="/"><img class="logo" src="/images/logo.png" alt="" /></a>
    <!-- <form class="prof-src" action="/">
      <input class="prof-src__input" type="text">
      <input type="submit" value="Search">
    </form> -->
    <nav>
      <% if(userInfo == false) { %>
        <li><a href="/login">Login</a></li>
        <li><a href="/register">Register</a></li>
        <% } else { %>
          <% const userImage = userInfo.profileImage ? userInfo.profileImage.replace(/\\/g, "/" ) : '' %>
            <li><a href="/dashboard">Dashboard</a></li>
            <li><a href="/settings">Settings</a></li>
            <li><a href="/logout">Logout</a></li>
            <a class="prof-go" href="/<%= userInfo.username %>">
              <div id="profile-img-display" style="background-image: url('/<%= userImage %>')"></div>
            </a>
            <% } %>
    </nav>
  </div>
</header>