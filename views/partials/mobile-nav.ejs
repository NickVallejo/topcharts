<div class="mobile-controls">
  <div class="mobile-nav">
    <div class="mobile-nav-wrap">
      <div class="ico-group">
        <i class="fas fa-list fa-2x"></i>
        <i class="fas fa-music fa-2x"></i>
      </div>

      <div class="round-up">
        <i class="fas fa-chevron-up fa-2x"></i>
      </div>

      <div class="ico-group">
        <i class="fas fa-cog fa-2x"></i>
        <div
          id="profile-img-display"
          class="prof-img-mob"
          style="background-image: url('http://143.198.119.208:8080/<%= userImage %>')"
        ></div>
      </div>
    </div>
  </div>

  <div class="mob-settings mob-slider profile-slide">
    <ul>
      <li class="dash-li"><a href="/dashboard">Dashboard</a></li>
      <li class="profile-li"><a href="/<%= userInfo.username %>">Profile</a></li>
      <li class="settings-li"><a href="/settings">Settings</a></li>
      <li class="logout-li"><a href="/logout">Logout</a></li>
    </ul>
  </div>

  <div class="mob-settings mob-slider setting-slide">
    <ul>
      <li class="save">Save</li>
      <li class="clear">New</li>
      <label for="mob-numRadio"
        ><li>
          Numbers?
          <input id="mob-numRadio" type="checkbox" checked="true" class="chartNums" name="chartNums" value="true" /></li
      ></label>
      <li class="screencap-btn">Download Image</li>
    </ul>
  </div>
</div>

<script type="text/javascript" src="http://143.198.119.208:8080/topcharts/public/js/interactive.js"></script>
